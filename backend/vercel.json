{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json", 
        "use": "@vercel/static-build",
        "config": {
          "distDir": "frontend/build"   
        }
      },
      {
        "src": "server/package.json",    
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/frontend/$1"  
      }
    ]
  }
  